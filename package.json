{
  "name": "papers",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "storybook": "sb-rn-get-stories && cross-env STORYBOOK_ENABLED='true' expo start",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "check:writing": "alex .",
    "generate-types": "supabase gen types typescript --project-id cgyqacvabseqensdfyzx > src/services/supabase/types/generated/database.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "pull:env": "dotenv-vault pull"
  },
  "dependencies": {
    "@react-native-community/netinfo": "9.3.10",
    "@supabase/supabase-js": "2.32.0",
    "@tanstack/react-query": "4.32.6",
    "expo": "49.0.7",
    "expo-constants": "14.4.2",
    "expo-router": "2.0.0",
    "expo-secure-store": "12.3.1",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-url-polyfill": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@storybook/addon-ondevice-actions": "6.5.6",
    "@storybook/addon-ondevice-backgrounds": "6.5.6",
    "@storybook/addon-ondevice-controls": "6.5.6",
    "@storybook/addon-ondevice-notes": "6.5.6",
    "@storybook/cli": "7.2.2",
    "@storybook/react": "7.2.2",
    "@storybook/react-native": "6.5.6",
    "@storybook/types": "7.2.2",
    "@tanstack/eslint-plugin-query": "4.32.5",
    "@testing-library/react-native": "12.2.0",
    "@types/jest": "29.5.3",
    "@types/react": "18.2.20",
    "alex": "11.0.0",
    "cross-env": "7.0.3",
    "dotenv-vault": "1.25.0",
    "eslint": "8.46.0",
    "eslint-config-universe": "11.3.0",
    "eslint-plugin-log-filenames": "1.0.6",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-expo": "49.0.0",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "tailwindcss": "3.3.3",
    "twrnc": "3.6.4",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{md,mdx,txt,html}": [
      "prettier --write",
      "alex"
    ]
  },
  "engines": {
    "node": "18.16.1",
    "npm": "This project is configured to use pnpm"
  },
  "packageManager": "pnpm@8.6.12"
}
